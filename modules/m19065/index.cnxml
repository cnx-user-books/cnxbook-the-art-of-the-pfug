            <m:mo>=</m:mo>
            <m:mn>0</m:mn>
          </m:mrow>
        </m:math>
      </equation>
      <para id="id2259301">and by (<link target-id=""/>)</para>
      <equation id="id2259309">
        <m:math mode="display">
