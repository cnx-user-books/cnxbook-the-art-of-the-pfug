              </m:mrow>
              <m:mi>ν</m:mi>
            </m:munderover>
            <m:msub>
              <m:mi>g</m:mi>
              <m:mi>i</m:mi>
            </m:msub>
            <m:mo>=</m:mo>
            <m:munderover>
