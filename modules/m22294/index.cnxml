            </row>
            <row>
              <entry>
                <m:math>
                  <m:msubsup>
                    <m:mi>I</m:mi>
                    <m:mi>i</m:mi>
                    <m:mrow>
                      <m:mi>e</m:mi>
                      <m:mi>x</m:mi>
                      <m:mi>t</m:mi>
                    </m:mrow>
                  </m:msubsup>
                </m:math>
              </entry>
