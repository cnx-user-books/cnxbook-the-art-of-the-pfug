            <row>
              <entry>
                <m:math>
                  <m:msubsup>
                    <m:mi>I</m:mi>
                    <m:mi>i</m:mi>
                    <m:mrow>
                      <m:mi>n</m:mi>
                      <m:mi>e</m:mi>
                      <m:mi>t</m:mi>
                    </m:mrow>
