                <m:mi>ϵ</m:mi>
                <m:mi>η</m:mi>
                <m:mo>)</m:mo>
              </m:mrow>
              <m:mo>+</m:mo>
              <m:msup>
                <m:mrow>
                  <m:mo>(</m:mo>
