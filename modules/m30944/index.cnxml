                          </m:msub>
                          <m:mo>)</m:mo>
                        </m:mrow>
                        <m:mn>2</m:mn>
                      </m:msup>
                      <m:mo>+</m:mo>
                      <m:msup>
                        <m:mrow>
                          <m:mo>(</m:mo>
                          <m:msub>
                            <m:mi>ϕ</m:mi>
                            <m:mi>t</m:mi>
                          </m:msub>
                          <m:mo>+</m:mo>
                          <m:mi>ϵ</m:mi>
