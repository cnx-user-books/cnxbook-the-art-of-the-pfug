                <m:mi>η</m:mi>
                <m:mn>2</m:mn>
              </m:msup>
              <m:mi>c</m:mi>
              <m:mi>o</m:mi>
              <m:mi>s</m:mi>
              <m:mrow>
                <m:mo>(</m:mo>
                <m:mn>2</m:mn>
                <m:mi>ϕ</m:mi>
                <m:mo>)</m:mo>
              </m:mrow>
              <m:mo>+</m:mo>
              <m:mn>2</m:mn>
              <m:msubsup>
                <m:mi>η</m:mi>
                <m:mi>θ</m:mi>
                <m:mn>2</m:mn>
              </m:msubsup>
              <m:mo>+</m:mo>
              <m:mn>2</m:mn>
              <m:msubsup>
                <m:mi>η</m:mi>
                <m:mi>t</m:mi>
