                  <m:mo>+</m:mo>
                  <m:mi>ϵ</m:mi>
                  <m:msub>
                    <m:mi>η</m:mi>
                    <m:mi>t</m:mi>
                  </m:msub>
                  <m:mo>)</m:mo>
                </m:mrow>
                <m:mn>2</m:mn>
              </m:msup>
              <m:mi>d</m:mi>
              <m:mi>θ</m:mi>
              <m:mi>d</m:mi>
