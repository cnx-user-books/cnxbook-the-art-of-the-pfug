                        <m:mi>t</m:mi>
                      </m:mrow>
                    </m:msub>
                    <m:mo>+</m:mo>
                    <m:msub>
                      <m:mi>β</m:mi>
                      <m:mrow>
                        <m:mi>e</m:mi>
                        <m:mo>,</m:mo>
                        <m:mn>2</m:mn>
                      </m:mrow>
                    </m:msub>
                    <m:mi>P</m:mi>
                    <m:mi>r</m:mi>
                    <m:mi>i</m:mi>
                    <m:mi>c</m:mi>
                    <m:msub>
                      <m:mi>e</m:mi>
                      <m:mrow>
                        <m:mi>e</m:mi>
                        <m:mo>,</m:mo>
                        <m:mi>t</m:mi>
                      </m:mrow>
                    </m:msub>
                    <m:mo>+</m:mo>
                    <m:msub>
                      <m:mi>β</m:mi>
                      <m:mrow>
                        <m:mi>e</m:mi>
                        <m:mo>,</m:mo>
                        <m:mn>3</m:mn>
                      </m:mrow>
                    </m:msub>
                    <m:mi>D</m:mi>
                    <m:mi>i</m:mi>
                    <m:mi>s</m:mi>
                    <m:msub>
                      <m:mi>p</m:mi>
                      <m:mrow>
                        <m:mi>b</m:mi>
                        <m:mo>,</m:mo>
                        <m:mi>t</m:mi>
                      </m:mrow>
                    </m:msub>
                    <m:mo>+</m:mo>
                    <m:msub>
                      <m:mi>β</m:mi>
                      <m:mrow>
                        <m:mi>e</m:mi>
                        <m:mo>,</m:mo>
                        <m:mn>4</m:mn>
                      </m:mrow>
                    </m:msub>
                    <m:mi>D</m:mi>
                    <m:mi>i</m:mi>
                    <m:mi>s</m:mi>
                    <m:msub>
                      <m:mi>p</m:mi>
                      <m:mrow>
                        <m:mi>e</m:mi>
                        <m:mo>,</m:mo>
                        <m:mi>t</m:mi>
                      </m:mrow>
                    </m:msub>
                    <m:mo>+</m:mo>
                    <m:msub>
                      <m:mi>β</m:mi>
                      <m:mrow>
                        <m:mi>e</m:mi>
                        <m:mo>,</m:mo>
                        <m:mn>5</m:mn>
                      </m:mrow>
                    </m:msub>
                    <m:msub>
                      <m:mi>Y</m:mi>
                      <m:mrow>
                        <m:mi>b</m:mi>
                        <m:mo>,</m:mo>
                        <m:mi>t</m:mi>
                        <m:mo>-</m:mo>
                        <m:mn>1</m:mn>
                      </m:mrow>
                    </m:msub>
                    <m:mo>+</m:mo>
                    <m:msub>
                      <m:mi>β</m:mi>
                      <m:mrow>
                        <m:mi>e</m:mi>
                        <m:mo>,</m:mo>
                        <m:mn>6</m:mn>
                      </m:mrow>
                    </m:msub>
                    <m:msub>
                      <m:mi>Y</m:mi>
                      <m:mrow>
                        <m:mi>e</m:mi>
                        <m:mo>,</m:mo>
                        <m:mi>t</m:mi>
                        <m:mo>-</m:mo>
