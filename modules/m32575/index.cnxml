                    <m:msqrt>
                      <m:mi>k</m:mi>
                    </m:msqrt>
                  </m:mfrac>
                  <m:mi>x</m:mi>
                  <m:mo>+</m:mo>
                  <m:msub>
                    <m:mi>b</m:mi>
                    <m:mrow>
                      <m:mn>1</m:mn>
                      <m:mo>,</m:mo>
                      <m:mn>2</m:mn>
