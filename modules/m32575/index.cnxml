                  <m:mi>u</m:mi>
                  <m:mn>21</m:mn>
                </m:msub>
              </m:mtd>
              <m:mtd>
                <m:mo>=</m:mo>
              </m:mtd>
              <m:mtd columnalign="left">
                <m:mrow>
                  <m:msub>
                    <m:mi>a</m:mi>
                    <m:mn>2</m:mn>
                  </m:msub>
                  <m:mo form="prefix">cos</m:mo>
                  <m:mi>λ</m:mi>
                  <m:mi>x</m:mi>
                  <m:mo>+</m:mo>
                  <m:msub>
                    <m:mi>b</m:mi>
                    <m:mn>2</m:mn>
                  </m:msub>
                  <m:mo form="prefix">sin</m:mo>
                  <m:mi>λ</m:mi>
                  <m:mi>x</m:mi>
                  <m:mo>+</m:mo>
                  <m:msub>
                    <m:mi>α</m:mi>
                    <m:mn>2</m:mn>
                  </m:msub>
                  <m:mo form="prefix">cos</m:mo>
                  <m:mfrac>
                    <m:mi>λ</m:mi>
                    <m:msqrt>
                      <m:mi>k</m:mi>
                    </m:msqrt>
                  </m:mfrac>
                  <m:mi>x</m:mi>
                  <m:mo>+</m:mo>
                  <m:msub>
                    <m:mi>β</m:mi>
                    <m:mn>2</m:mn>
                  </m:msub>
                  <m:mo form="prefix">sin</m:mo>
                  <m:mfrac>
                    <m:mi>λ</m:mi>
                    <m:msqrt>
                      <m:mi>k</m:mi>
                    </m:msqrt>
                  </m:mfrac>
                  <m:mi>x</m:mi>
                </m:mrow>
              </m:mtd>
            </m:mtr>
            <m:mtr>
              <m:mtd columnalign="right">
                <m:msub>
                  <m:mi>u</m:mi>
                  <m:mn>22</m:mn>
                </m:msub>
              </m:mtd>
              <m:mtd>
                <m:mo>=</m:mo>
              </m:mtd>
              <m:mtd columnalign="left">
                <m:mrow>
                  <m:msub>
                    <m:mi>a</m:mi>
                    <m:mn>2</m:mn>
                  </m:msub>
                  <m:mo form="prefix">cos</m:mo>
                  <m:mi>λ</m:mi>
                  <m:mi>x</m:mi>
                  <m:mo>+</m:mo>
                  <m:msub>
                    <m:mi>b</m:mi>
                    <m:mn>2</m:mn>
                  </m:msub>
                  <m:mo form="prefix">sin</m:mo>
                  <m:mi>λ</m:mi>
                  <m:mi>x</m:mi>
                  <m:mo>-</m:mo>
                  <m:msub>
                    <m:mi>α</m:mi>
                    <m:mn>2</m:mn>
                  </m:msub>
                  <m:mo form="prefix">cos</m:mo>
                  <m:mfrac>
                    <m:mi>λ</m:mi>
                    <m:msqrt>
                      <m:mi>k</m:mi>
                    </m:msqrt>
                  </m:mfrac>
                  <m:mi>x</m:mi>
                  <m:mo>-</m:mo>
                  <m:msub>
                    <m:mi>β</m:mi>
                    <m:mn>2</m:mn>
                  </m:msub>
                  <m:mo form="prefix">sin</m:mo>
                  <m:mfrac>
                    <m:mi>λ</m:mi>
                    <m:msqrt>
                      <m:mi>k</m:mi>
                    </m:msqrt>
                  </m:mfrac>
                  <m:mi>x</m:mi>
