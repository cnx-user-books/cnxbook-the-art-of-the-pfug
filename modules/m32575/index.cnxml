                  <m:mfrac>
                    <m:mn>1</m:mn>
                    <m:mn>2</m:mn>
                  </m:mfrac>
                  <m:mrow>
                    <m:mo>(</m:mo>
                    <m:mi>k</m:mi>
                    <m:mo>+</m:mo>
                    <m:mn>1</m:mn>
                    <m:mo>)</m:mo>
                  </m:mrow>
                  <m:mfenced separators="" open="(" close=")">
                    <m:msub>
                      <m:mi>b</m:mi>
                      <m:mn>2</m:mn>
                    </m:msub>
                    <m:mi>λ</m:mi>
                    <m:mo>-</m:mo>
                    <m:msub>
                      <m:mi>β</m:mi>
                      <m:mn>2</m:mn>
                    </m:msub>
                    <m:mfrac>
                      <m:mi>λ</m:mi>
                      <m:msqrt>
                        <m:mi>k</m:mi>
                      </m:msqrt>
                    </m:mfrac>
                  </m:mfenced>
                  <m:mo>-</m:mo>
                  <m:mfrac>
