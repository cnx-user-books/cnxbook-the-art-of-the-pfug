                  </m:mphantom>
                  <m:mfenced separators="" open="" close=")">
                    <m:mspace width="2.em"/>
                    <m:mo>-</m:mo>
                    <m:mfrac>
                      <m:msup>
                        <m:mfenced separators="" open="(" close=")">
