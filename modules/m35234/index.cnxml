                        <m:mo>+</m:mo>
                        <m:mi>π</m:mi>
                        <m:msub>
                          <m:mi>μ</m:mi>
                          <m:mn>1</m:mn>
                        </m:msub>
                        <m:mi> cos </m:mi>
                        <m:mrow>
                          <m:mo>(</m:mo>
                          <m:mi>π</m:mi>
                          <m:mi>t</m:mi>
                          <m:mo>)</m:mo>
                        </m:mrow>
                        <m:mi>f</m:mi>
                        <m:mrow>
                          <m:mo>(</m:mo>
                          <m:mi>t</m:mi>
                          <m:mo>)</m:mo>
                        </m:mrow>
                        <m:mo>+</m:mo>
                        <m:msub>
                          <m:mi>μ</m:mi>
