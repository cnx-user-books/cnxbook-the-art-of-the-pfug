                    <m:mo>(</m:mo>
                    <m:msup>
                      <m:mi>x</m:mi>
                      <m:mn>2</m:mn>
                    </m:msup>
                    <m:msup>
                      <m:mi>z</m:mi>
                      <m:mn>2</m:mn>
                    </m:msup>
                    <m:mo>+</m:mo>
                    <m:mi>y</m:mi>
                    <m:msup>
                      <m:mi>z</m:mi>
                      <m:mn>3</m:mn>
                    </m:msup>
                    <m:mo>)</m:mo>
                  </m:mrow>
                  <m:mo>=</m:mo>
                  <m:mo>-</m:mo>
                  <m:msup>
                    <m:mi>y</m:mi>
                    <m:mn>4</m:mn>
                  </m:msup>
                  <m:msup>
                    <m:mi>z</m:mi>
                    <m:mn>3</m:mn>
                  </m:msup>
