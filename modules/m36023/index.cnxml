                  <m:msup>
                    <m:mi>x</m:mi>
                    <m:mn>3</m:mn>
                  </m:msup>
                  <m:msup>
                    <m:mi>z</m:mi>
                    <m:mn>3</m:mn>
                  </m:msup>
